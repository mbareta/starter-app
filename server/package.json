{
  "name": "nest-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./src",
    "pretest": "NODE_ENV=test npx mikro-orm schema:fresh --run --seed",
    "test": "NODE_ENV=test jest || true",
    "posttest": "NODE_ENV=test npx mikro-orm schema:drop -r",
    "test:watch": "jest --watch",
    "test:cov": "npm run pretest && jest --coverage && npm run posttest",
    "test:debug": "npm run pretest && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.2.9",
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/nestjs": "^5.2.3",
    "@mikro-orm/postgresql": "^6.2.9",
    "@mikro-orm/seeder": "^6.2.9",
    "@nest-middlewares/response-time": "^10.0.0",
    "@nestjs/class-transformer": "^0.4.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.4",
    "auth0": "^4.7.0",
    "class-validator": "^0.14.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "on-finished": "^2.4.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "eslint": "^9.12.0",
    "eslint-config-nestjs": "^0.8.0",
    "jest": "^29.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
